      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/profiles/model/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/profiles/model"
          ],
          "options": {
            "jestConfig": "libs/profiles/model/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
